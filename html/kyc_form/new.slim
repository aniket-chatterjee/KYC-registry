    selectMonths: 1, // Creates a dropdown to control month
    selectYears: 1,// Creates a dropdown of 15 years to control year,
    clear: 'Clear',
    close: 'Ok',
    min: new Date(),
    closeOnSelect: false // Close upon selecting a date,
  });
  function submit(e){
    if(validateAll())
      $('form#new_form').submit();
  }

  function validateAll(){
      var flag=true;
      var occupation_type_children={
        "Service":['Private Sector','Public Sector', 'Government Sector'],
        "Others":['Professional','Self Employed', 'Retired','Housewife','Student']
      }
      var document_types=["passport","voter_id","driving_licence","aadhaar_card","nrega_job_card","other_card"]
      for(var i=0;i<required_fields.length;i++){
          var f=$("[name='"+required_fields[i]+"']");

          if(f.attr('type')==="radio")
            {
              v=$('[name="'+required_fields[i]+'"]:checked').val();
              if($('[name="'+required_fields[i]+'"]:checked').length<1)
                {
                  flag=false;
                  f.next("label").addClass('red-text');
                }
              else if(required_fields[i]=="occupation_type" && (v=="Service" || v=="Others"))
              {
                  if(f.next("[name='occupation_sector']:checked").length<1)
                    $("[name='occupation_sector']").next("label").addClass('red-text');
                  else if(occupation_type_children[v].indexOf(f.next("[name='occupation_sector']:checked").val())==-1)
                      f.next("[name='occupation_sector']:checked").next("label").addClass('red-text');
                  else
                    f.next("[name='occupation_sector']:checked").next("label").removeClass('red-text');
              }
              else if(required_fields[i]=="kyc_type" && v=="normal"){
                if($("#pan_number").val().trim().length<1){
                  $("#pan_number").next("label").prop('data-error','required');
                  $("#pan_number").removeClass("valid").addClass("invalid");
                }
              }
              else
                f.next("label").removeClass('red-text');

            }
          else{
              if(!v || v=="" || v.trim().length<1){
                f.next("label").prop('data-error','required');
                f.removeClass("valid");
                f.addClass("invalid");
                flag=false;
              }
            }
      }
      return flag;
  }
