javascript:
  var required_fields=["kyc_type", "name_prefix", "first_name", "last_name", "father_spouse_name_prefix", "father_spouse_first_name", "father_spouse_last_name", "mother_name_prefix", "mother_first_name", "mother_last_name", "date_of_birth", "address_line1", "city_town_village", "district", "zip_pin", "state_ut","gender","marital_status","country","residential_status","occupation_type","proof_of_identity_type"];
  var only_character_fields=["name_prefix", "first_name", "last_name", "father_spouse_name_prefix", "father_spouse_first_name", "father_spouse_last_name", "mother_name_prefix", "mother_first_name", "mother_last_name","related_person_name_prefix","related_person_first_name","related_person_middle_name","related_person_last_name"];
  var max_limit_fields={pan_number:12,name_prefix:3,  first_name:11,  middle_name:11,  last_name:11,  maiden_name_name_prefix:3,  maiden_name_first_name:11,  maiden_name_middle:11,  maiden_name_last_name:11,  father_spouse_name_prefix:3,  father_spouse_first_name:11,  father_spouse_middle_name:11,  father_spouse_last_name:11,  mother_name_prefix:3,  mother_first_name:11,  mother_middle_name:11,  mother_last_name:11,  passport_number:9,  voter_id_number:12,  driving_licence_number:15,  aadhaar_card_number:15,nrega_job_card_number:14,  other_card_name:10,  other_card_number:15,  address_line1:46,  address_line2:46,  address_line3:29,  city_town_village:9,  district:10,  zip_pin:6,  state_ut_code:2,  state_ut:16,  email_id:45,  mobile:13,  telephone_off:13,  telephone_res:13,  }
  $(document).ready(function() {
      $('select').material_select();

        for(var i in only_character_fields)
          $("input[name='"+only_character_fields[i]+"']").on("keyup keypress blur change",characterCheck);
        for(var i in max_limit_fields)
          $("input[name='"+i+"']").on("keyup keydown change",{max:max_limit_fields[i]},maxLengthCheck);
    });
    function characterCheck(event){
        var val=event.target.value;
        console.log()
        while(!val.match(/^[a-zA-Z]*$/)){
          val=val.slice(0,-1);
        }
        event.target.value=val;
    }
    function maxLengthCheck(event){
        var val=event.target.value;
        console.log()
        while(val.length>event.data.max){
          val=val.slice(0,-1);
        }
        $(event.target).val(val);
        //$(event.target).focus();
        //if(event.type=="blur"){
        //  $(event.target).blur();
        //}
    }
  $('.collapsible').collapsible('open', 0);
  $('#date_of_birth').pickadate({
